<nav class="navbar subnav-box p-t-1 p-b-1">
  <div class="container">
    <a class="subnav-control" href="{{href-to 'index'}}">All Files <span class="octicon octicon-triangle-down"></span></a>
  </div>
</nav>

<div class="container">
  <div class="row">
    <div class="col-sm-12 col-md-9">
      
      <div class="btn-toolbar m-b-1" role="toolbar" aria-label="Toolbar with button groups">
        <div class="btn-group" role="group" aria-label="First group">
          <button type="button" class="btn btn-box"><span class="octicon octicon-cloud-upload"></span> Upload</button>
        </div>
        <div class="btn-group" role="group" aria-label="First group">
          <button type="button" class="btn btn-box-default"><span class="octicon octicon-plus"></span> New</button>
        </div>
        <div class="btn-group" role="group" aria-label="First group">
          <button type="button" class="btn btn-box-default"><span class="octicon octicon-pencil"></span></button>
        </div>
        <div class="btn-group pull-xs-right" role="group" aria-label="Third group">
          <div class="checkbox">
            <label class="p-t-xs">
              <input type="checkbox">
            </label>
          </div>
        </div>
        <div class="btn-group pull-xs-right" role="group" aria-label="Third group">
          <button type="button" class="btn btn-box-default"><span class="octicon octicon-unfold"></span></button>
        </div>
      </div>

      <ul class="media-list asset-list">
        {{#each model as |p|}}
          {{asset-list-item p=p}}
        {{/each}}
      </ul>

      {{!-- {{uploader-form}} --}}

    </div>

    <div class="col-sm-12 col-md-3">
      One of three columns
    </div>
  </div>
</div>
